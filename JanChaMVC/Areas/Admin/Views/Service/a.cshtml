                        <script type="text/javascript">
                            $(function () {
                                $(".gao-server-style .gao-left li").eq(0).find("dl").slideDown();
                                var id = "";
                                var name = "";

                                function auto() {
                                    var th = $(".gao-server-style .gao-left li dl dd").eq(0);
                                    var tit = th.find("a").text();

                                    id = th.find("a").attr("id");
                                    name = th.find("a").attr("name");

                                    th.addClass("on").siblings("dd").removeClass("on");
                                    $(".gao-server-style .gao-right .gao-b-t .t").text(tit);

                                    ajax_fun(id, name);
                                }
                                function ajax_fun(id, name) {

                                    var url = "getcontent";

                                    $.ajax({
                                        url: url,
                                        data: { "id": id, "name": name },
                                        type: "POST",
                                        dataType: "json",
                                        success: function (data) {

                                            $(".gao-server-style .gao-right .gao-b-t .gao-con div.gao-contents").html(data);

                                        },
                                        complete: function (XMLHttpRequest, textStatus) {

                                            $(".gao-server-style .gao-right .gao-b-t .gao-con div.gao-contents").html(XMLHttpRequest.responseText);

                                        },
                                        error: function (data) {
                                            // 			    	console.log("error")
                                        }
                                    });
                                };
                                auto();
                                $(".gao-server-style .gao-left li p").click(function () {

                                    $(".gao-server-style .gao-left li dl").slideUp();
                                    $(this).siblings("dl").slideDown();

                                })
                                $(".gao-server-style .gao-left li dl dd").click(function () {

                                    $(".gao-server-style .gao-right .gao-b-t .gao-con div.gao-contents").css("height", "158px");
