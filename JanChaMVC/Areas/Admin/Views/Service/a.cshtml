                                    // var con = $(this).find("span.gao-content").html();
                                    var tit = $(this).find("a").text();
                                    // var href = $(this).find("a").attr("href");

                                    id = $(this).find("a").attr("id");
                                    name = $(this).find("a").attr("name");
                                    ajax_fun(id, name);

                                    $(this).addClass("on").siblings("dd").removeClass("on");
                                    $(".gao-server-style .gao-right .gao-b-t .t").text(tit);
